                ],
                "summary": "User registration",
                "operationId": "1f6d809ab0bc795fb709452e12243084",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password"
